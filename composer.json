{
    "name": "viewtrender/php-youtube-testkit-laravel",
    "description": "Laravel integration for php-youtube-testkit",
    "type": "library",
    "license": "MIT",
    "version": "0.5.0",
    "require": {
        "php": "^8.3",
        "viewtrender/php-youtube-testkit-core": "^0.3",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^10.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Viewtrender\\Youtube\\Laravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Viewtrender\\Youtube\\Laravel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Viewtrender\\Youtube\\Laravel\\YoutubeDataApiServiceProvider"
            ],
            "aliases": {
                "YoutubeDataApi": "Viewtrender\\Youtube\\Laravel\\Facades\\YoutubeDataApi",
                "YoutubeAnalyticsApi": "Viewtrender\\Youtube\\Laravel\\Facades\\YoutubeAnalyticsApi",
                "YoutubeReportingApi": "Viewtrender\\Youtube\\Laravel\\Facades\\YoutubeReportingApi"
            }
        }
    }
}
